<script type="text/javascript">
  function initialize() {
    var mapOptions = {
      center: new google.maps.LatLng(39.951719, -75.163877),
      zoom: 14,
      mapTypeId: google.maps.MapTypeId.ROADMAP
    };

    var map = new google.maps.Map(document.getElementById("map-canvas"), mapOptions);

    var marker = new google.maps.Marker({
      position: new google.maps.LatLng(39.947697,-75.15008),
      map: map
    });

    var marker = new google.maps.Marker({
      position: new google.maps.LatLng(39.94745,-75.149233),
      map: map
    });

    <% for startup in @startups %>
        var marker = new google.maps.Marker({
          position: new google.maps.LatLng(<%= startup.x.to_f  %>, <%= startup.y.to_f  %>),
          map: map
        });
    <% end  %>

  }

  google.maps.event.addDomListener(window, 'load', initialize);
</script>


